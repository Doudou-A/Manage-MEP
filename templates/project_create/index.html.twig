{% extends "base.html.twig" %}

{% block body %}
	<div class="col-md-9 border p-4 bg-white mx-auto">

		<h1 class="p-2 text-center font-MF border-bottom border-primary">Inscription</h1>

		{{ form_start(form) }}
		{{ form_row(form.name, { 'label': 'Nom du projet'}) }}
		<div class="row justify-content-center">
			<button type="submit" class="btn btn-success my-3 font-MF">Inscription !
			</button>
		</div>
		{{ form_end(form)}}
	</div>
{% endblock %}
